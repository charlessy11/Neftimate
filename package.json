  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --build",
    "clean": "rm -rf lib ./lambda/*/lib ./scripts/*/lib",
    "migrate": "knex migrate:latest",
    "dev:add-collection": "npm run build && node scripts/collection-data-poller/lib/index.js",
